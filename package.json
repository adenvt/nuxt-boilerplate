{
  "name": "nuxt-boilerplate",
  "version": "0.2.0",
  "description": "My remarkable Nuxt.js project",
  "author": "Ade Novid",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint 'assets/**/*.scss' --syntax scss",
    "fix": "npm run fix:js && npm run fix:css",
    "fix:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "fix:css": "stylelint 'assets/**/*.scss' --syntax scss --fix"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/proxy": "^1.3.1",
    "animate.css": "^3.7.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-beta",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "nuxt": "^2.0.0",
    "nuxt-simple-line-icons": "^1.0.1",
    "vuex-easy-access": "^3.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.10.0",
    "eslint-config-adenvt": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "eslint-plugin-varspacing": "^1.2.1",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-scss": "^3.4.1"
  }
}
