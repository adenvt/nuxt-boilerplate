{
  "name": "nuxt-boilerplate",
  "version": "0.2.0",
  "description": "My remarkable Nuxt.js project",
  "author": "Ade Novid",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint 'assets/**/*.scss' --syntax scss",
    "fix": "npm run fix:js && npm run fix:css",
    "fix:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "fix:css": "stylelint 'assets/**/*.scss' --syntax scss --fix"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/proxy": "^1.3.3",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.27",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.8.1",
    "nuxt-simple-line-icons": "^1.0.2",
    "vuejs-datepicker": "^1.6.2",
    "vuex-easy-access": "^3.1.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.1.0",
    "eslint-config-adenvt": "^4.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "eslint-plugin-varspacing": "^1.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-scss": "^3.9.2"
  }
}
